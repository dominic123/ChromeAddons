{
  "manifest_version": 3,
  "name": "SharePoint Field Creator from CSV",
  "version": "1.0.0",
  "description": "Upload CSV file to create fields in SharePoint 2019 on-premises lists using JSOM",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["jsom_injector.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
